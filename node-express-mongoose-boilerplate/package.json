{
  "name": "express-mongoose-boilerplate",
  "version": "1.0.0",
  "description": "RESTful API Node Server Boilerplate",
  "directories": {
    "lib": "src"
  },
  "main": "lib",
  "scripts": {
    "build:clean": "rimraf lib",
    "build": "babel src -d lib",
    "doc": "documentation build src/** --format html --github --document-exported --output doc/api",
    "prebuild": "npm run build:clean",
    "start": "node-dev src",
    "start-prod": "NODE_ENV=production npm run build & node lib",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git://toanhm-1842/node-express-mongoose-boilerplate.git"
  },
  "author": "Sun* NodeJS Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toanhm-1842/node-express-mongoose-boilerplate/issues"
  },
  "homepage": "https://github.com/toanhm-1842/node-express-mongoose-boilerplate#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "acorn": "^7.1.1",
    "ajv": "^6.12.0",
    "bluebird": "^3.7.2",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "forever": "^2.0.0",
    "helmet": "^3.21.3",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "moment": "2",
    "moment-timezone": "0.5",
    "mongoose": "^5.8.11",
    "morgan": "^1.10.0",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "source-map-support": "^0.5.16",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
